<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ProjectCoder.ViewModel"
                    xmlns:view="clr-namespace:ProjectCoder.View">

    <DataTemplate DataType="{x:Type vm:CoursesVM}">
        <view:Courses/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Settings/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UserVM}">
        <view:User/>
    </DataTemplate>   

</ResourceDictionary>